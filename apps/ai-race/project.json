{
    "name": "ai-race",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/ai-race",
    "projectType": "application",
    "tags": [
        "type:app",
        "scope:dashboard"
    ],
    "targets": {
        "build": {
            "executor": "@nx/web:webpack",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/apps/ai-race",
                "index": "apps/ai-race/AI_RACE_CLEAN-main/index.html",
                "main": "apps/ai-race/AI_RACE_CLEAN-main/scraper.py",
                "tsConfig": "apps/ai-race/tsconfig.app.json",
                "assets": [
                    "apps/ai-race/AI_RACE_CLEAN-main/manifest.json",
                    "apps/ai-race/AI_RACE_CLEAN-main/sw.js",
                    "apps/ai-race/AI_RACE_CLEAN-main/styles.css",
                    "apps/ai-race/AI_RACE_CLEAN-main/favicon.ico"
                ]
            }
        },
        "serve": {
            "executor": "@nx/web:dev-server",
            "options": {
                "buildTarget": "ai-race:build",
                "port": 4200
            }
        },
        "deploy": {
            "executor": "nx:run-commands",
            "options": {
                "command": "gh-pages -d dist/apps/ai-race -r https://github.com/yourusername/ai-race.git"
            }
        }
    },
    "implicitDependencies": [
        "shared-keys",
        "shared-pwa",
        "data-layer"
    ]
}