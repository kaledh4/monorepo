{
    "name": "intelligence-platform",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/intelligence-platform",
    "projectType": "application",
    "tags": [
        "type:app",
        "scope:dashboard"
    ],
    "targets": {
        "build": {
            "executor": "@nx/web:webpack",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/apps/intelligence-platform",
                "index": "apps/intelligence-platform/index.html",
                "main": "apps/intelligence-platform/app.js",
                "tsConfig": "apps/intelligence-platform/tsconfig.app.json",
                "assets": [
                    "apps/intelligence-platform/manifest.json",
                    "apps/intelligence-platform/service-worker.js",
                    "apps/intelligence-platform/styles.css",
                    "apps/intelligence-platform/icons"
                ]
            }
        },
        "serve": {
            "executor": "@nx/web:dev-server",
            "options": {
                "buildTarget": "intelligence-platform:build",
                "port": 4204
            }
        },
        "deploy": {
            "executor": "nx:run-commands",
            "options": {
                "command": "gh-pages -d dist/apps/intelligence-platform -r https://github.com/yourusername/intelligence-platform.git"
            }
        }
    },
    "implicitDependencies": [
        "shared-keys",
        "shared-pwa",
        "data-layer"
    ]
}